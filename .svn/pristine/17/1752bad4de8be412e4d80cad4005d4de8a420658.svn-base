#import "CCPlayerTools.h"

@implementation CCPlayerTools

+ (int)convertToMinuteWith:(CGFloat)time {
    return time / 60;
}

+ (int)convertToSecondsWith:(CGFloat)time {
    int seconds = (int)time;
    return seconds % 60;
}

@end
